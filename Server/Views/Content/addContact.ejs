<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Buissness Contact View</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="/css/addContact.css" rel="stylesheet">
    </head>
    <body>
<%- include('../Components/NavBar.ejs') %>
    <div class="addContact-container">
   
    <form action="/addContact" method="post" id="formPage">
        <div class="form-group">

            <input type="text" class="addContact-input" name="name" id="userName-input" placeholder="Name" />
        </div>
         <div class="form-group">

            <input type="text" class="addContact-input" name="email" id="email-input" placeholder="Email" />
        </div>
         <div class="form-group">

            <input type="text" class="addContact-input" name="cellphone" id="password-input" placeholder="CellPhone"/>
        </div>
     <div class="form-group">
        <input type="text" class="addContact-input" name="relation" id="password-input" placeholder="Relation"/>
        </div>
        <button type="submit" class="addContact-btn" >Create Contact</button>
         <button type="submit" class="btn btn-primary" >Cancel</button>
    </form>
    

    
    
    </div>
    


<%- include('../Components/footer.ejs') %>
    </body>
</html>